<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Target Name="CreateNuPkg">
	<MakeDir Directories="$(WebStackRootPath)nupkg" />
	<Exec WorkingDirectory="$(ProjectDir)" Command="$(WebStackRootPath).nuget\nuget.exe pack $(ProjectName).nuspec -OutputDirectory $(WebStackRootPath)nupkg" />
  </Target>
</Project>